{
    "name": "nhantamz/laravel-turnstile",
    "description": "Cloudflare Turnstile for Laravel",
    "keywords": [
        "cloudflare",
        "turnstile",
        "captcha",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "nhantamz",
            "email": "nhantamz@github.com"
        }
    ],
    "require": {
        "php": ">=5.5.5",
        "illuminate/support": "5.*|6.*|7.*|8.*|^9.0|10.*|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|^9.5.10|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nhantamz\\Turnstile\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nhantamz\\Turnstile\\TurnstileServiceProvider"
            ],
            "aliases": {
                "Turnstile": "Nhantamz\\Turnstile\\Facades\\Turnstile"
            }
        }
    }
}
